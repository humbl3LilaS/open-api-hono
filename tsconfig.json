{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@routes/*": ["./src/routes/*"],
      "@db/*": ["./src/db/*"],
      "@doc/*": ["./src/doc/*"],
      "@valid/*": ["./src/validators/*"],
      "@actions/*": ["./src/actions/*"],
      "@/*": ["./src/*"]
    },
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
